DECLARE @idNumCount INT = 100000;
DECLARE @usersNeededToBeInputted = SELECT INTEGER;

DECLARE @name TEXT = '';
DECLARE @username TEXT = '';
DECLARE @email TEXT = '';
DELCARE @age INTEGER = '';
DECLARE @idNum TEXT = '';

WHILE @count < usersNeededToBeInputted
BEGIN
	PRINT 'Added ' + name;
	
	@name = select(users) TEXT;
	@username = select(users) TEXT;
	@email = select(users) TEXT;
	@age = select(users) INTEGER;
	
	INSERT INTO users (name, username, email, age, idNum)
	VALUES (@name, @username, @email, @age, '#' + @idNumCount);
